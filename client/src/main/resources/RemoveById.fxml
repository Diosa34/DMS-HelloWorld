                </rowConstraints>
                <children>
                    <Label text="ID: " />
                    <JFXTextField fx:id="id" promptText="ID" GridPane.columnIndex="1" />
                    <Label fx:id="label" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
                </children>
            </GridPane>
        </header>
    </DialogPane>
    <JFXButton fx:id="readyButton" onAction="#removeByIdButtonHandle" layoutX="181.0" layoutY="108.0" mnemonicParsing="false" text="Remove" />
</AnchorPane>
